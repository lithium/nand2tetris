/**
 * And gate: out = 1 if {a==1 and b==1}, 0 otherwise
 * And gate: if {a==1 and b==1} then out = 1 else out = 0
 */

/*
A B | OUT
----+----
0 0 | 0
1 0 | 0
0 1 | 0
1 1 | 1
*/

CHIP And {
    IN a, b;
    OUT out;

    PARTS:
    Nand(a=a,b=b,out=w);
    Not(in=w,out=out);
}
